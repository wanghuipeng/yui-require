-webkit-border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;
border-width:0 0 0 1px;}
.input-number span{border-left:1px solid #e2e2e2;position: relative;border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;
-webkit-border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;
border-width:0 0 0 1px;}
.input-number span:first-child{border-left: none;border: none;}
.errortip{background-color:#eb4f38;color:#fff;position:fixed;z-index: 100;top:0;left:0;width:100%;height:45px;line-height:45px;text-align:center;display:none;}
.yui-textarea{margin-bottom: -10px;width:100%;border:medium none;resize:none;min-height:60px;line-height:20px;font-size: 14px;color: #666;}
.yui-textarea.bd{border: 1px solid #e2e2e2;border-radius: 0;border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;
-webkit-border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;
border-width: 1px;}
.textarea-count{position:relative;}
.textarea-count .count{position:absolute;right:0;bottom:0;color:#999}
.yui-select-sty{position: relative;width: 64%;float: left;}
.yui-select-sty .yui-select{position: absolute;width: 100%;padding:0px 8px;-webkit-user-select: text;margin-right: 15px;font-size: 14px;border-radius: 0;height: 30px;line-height: 30px;border: 1px solid #e2e2e2;border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;
-webkit-border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;
border-width: 1px;}
.yui-select-sty .arrow-select,.yui-select-wrapper>li:after{background: url(../images/arrow-select.png) no-repeat;width: 30px;height: 30px;display: inline-block;position: absolute;right:0px;top:0px;background-size: 30px;}
.form-input{-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;width:100%;padding:4px 8px;height:40px;line-height:40px;border:1px solid #e2e2e2;color:#999;font-size:14px;border-radius: 0px;border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;
-webkit-border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;
border-width: 1px;}
.form-input:focus{border-color: #2bb7ab;}
/*media*/
.media{background-color: #fff;padding: 10px 10px;border-bottom: 1px solid #e2e2e2;border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;-webkit-border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;border-width: 0 0 1px 0;}
.media-left,.media-body,.media-right,.media-arrow{display: table-cell;vertical-align: top;}
.media-body{width: 10000px;}
.media-left{padding-right: 10px;}
.media-right{padding-left: 10px;}
.media-img{font-size: 12px;width: 42px;height: 42px;background-color:#e2e2e2;border:none;vertical-align: middle;display: block;}
.media-arrow i{margin-top: 15px;}
/*progress*/
.progress{width: 100%;height: 5px;background-color: #e2e2e2;position: relative;}
.progress .progress-bar{width: auto;height: 5px;position: absolute;left: 0;top: 0;border-radius: 3px;}
.progress-sucess .progress-bar{background-color: #5cb85c;}
.progress-danger .progress-bar{background-color: #d9534f;}
.progress-warn .progress-bar{background-color: #f0ad4e;}
.progress.progress-large{height:8px}
.progress.progress-large .progress-bar{height:8px}
.progress-striped .progress-bar{background-image:-webkit-linear-gradient(45deg,rgba(255,255,255,.15) 25%,transparent 25%,transparent 50%,rgba(255,255,255,.15) 50%,rgba(255,255,255,.15) 75%,transparent 75%,transparent);background-image:-o-linear-gradient(45deg,rgba(255,255,255,.15) 25%,transparent 25%,transparent 50%,rgba(255,255,255,.15) 50%,rgba(255,255,255,.15) 75%,transparent 75%,transparent);background-image:linear-gradient(45deg,rgba(255,255,255,.15) 25%,transparent 25%,transparent 50%,rgba(255,255,255,.15) 50%,rgba(255,255,255,.15) 75%,transparent 75%,transparent);background-size:14px 15px;-moz-transition:width .6s ease;-webkit-transition:width .6s ease;-o-transition:width .6s ease;transition:width .6s ease}
/*tabs*/
.topTabs{height: 35px;background-color: white;border: 1px solid #ccc;line-height: 35px;}
.topTabs>ul{width: 100%;}
.topTabs>ul>li{display: inline-block;float: left;text-align: center;cursor: pointer;}
.topTabs>ul>li a{width: 100%;display: inline-block;}
.topTabs>ul>li:first-child{border-left: none;margin-left: 0;}
.topTabs>ul.tabs-4-item>li{width: 25%;}
.topTabs>ul.tabs-3-item>li{width: 33.33%;}
.topTabs>ul.tabs-2-item>li{width: 50%;}
.tab-contents .tab-item{display: none;}
.tab-contents .tab-item.current{display: block;}
.topTabs.borderTabs li{background-color:inherit !important;border-image:none;}
.topTabs.borderTabs li.current{color:#2bb7ab}
.slider-border{width:30%;height:2px;background-color:#2bb7ab;margin:auto;margin-top:-1px}
.current0{-webkit-transform:translateX(-80%);transform:translateX(-80%);-webkit-transition:transform .2s linear;transition:transform .2s linear;-moz-transition-duration:.2s;-webkit-transition-duration:.2s;-o-transition-duration:.2s;transition-duration:.2s}
.current1{-webkit-transform:translateX(80%);transform:translateX(80%);-webkit-transition:transform .2s linear;transition:transform .2s linear;-moz-transition-duration:.2s;-webkit-transition-duration:.2s;-o-transition-duration:.2s;transition-duration:.2s}
/*topTabs & footerTabs*/
.topTabs{height: 35px;background-color: white;border: 1px solid #e2e2e2;line-height: 35px;border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;-webkit-border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;border-width:1px 0 1px 0;}
.topTabs>ul,ul.tabs-4-item,ul.tabs-3-item,ul.tabs-2-item{width: 100%;}
.topTabs>ul>li,.footerTabs>ul>li{display: inline-block;float: left;text-align: center;border-left: 1px solid #e2e2e2;cursor: pointer;border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;
-webkit-border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;
border-width: 0 0 0 1px;}
.topTabs>ul>li:first-child{border-left: none;margin-left: 0;border-width: 0;}
.topTabs>ul>li.current,.topTabs > ul > li.active{background-color: #2bb7ab;color: white;}
.topTabs>ul>li.current a,.topTabs > ul > li.active a{color: white !important;}
ul.tabs-4-item>li{width: 25%;display: inline-block;float: left;box-sizing: border-box;}
ul.tabs-3-item>li{width: 33.33%;display: inline-block;float: left;box-sizing: border-box;}
ul.tabs-2-item>li{width: 50%;display: inline-block;float: left;box-sizing: border-box;}
.tab-contents{background-color: white;padding: 10px 15px;}
.tab-contents .tab-item{display: none;}
.tab-contents .tab-item.current{display: block;}
.footerTabs{height: 53px;background-color: white;}
.footerTabs>ul{position: fixed;left: 0;bottom: 0;width: 100%;background-color: white;border-top: 1px solid #e2e2e2;}
.footerTabs .footer-cell{height: 30px;}
