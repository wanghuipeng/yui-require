*{margin:0;padding:0;-webkit-overflow-scrolling: touch;outline:none;-webkit-tap-highlight-color:transparent;}
body{font:14px/1.5 'Microsoft Yahei','helvetica','arial';color:#666;-webkit-text-size-adjust:none;min-width:320px;line-height:1.5;font-weight:normal;}
h1,h2,h3,h4,h5,h6{font-size:100%}
form{display:inline}
ul,ol{list-style:none}
ul,li{margin: 0;padding: 0;}
a{text-decoration:none;color:#666;}
input,button,select,textarea{outline:none;table-select: rgba(0,0,0,0);tap-highlight-color: rgba(0,0,0,0);}
input[type="button"],input[type="submit"],input[type="text"]{-webkit-appearance: none;}
