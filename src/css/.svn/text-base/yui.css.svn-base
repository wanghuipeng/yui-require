.form-error .list-input{border-bottom: 1px solid #eb4f38;color: #666;}
.form-error .error-icon{background: url(../images/error.png) no-repeat;width: 20px;height: 20px;display: inline-block;position: absolute;top: 17px;left: 87%;background-size: 20px;}
.input-number{width: 92px;border: 1px solid #e2e2e2;display: inline-block;overflow: hidden;vertical-align: middle;border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;
-webkit-border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;
border-width: 1px;}
.input-number .add,.input-number .del{width:30px;height:30px;line-height:30px;text-align:center;display:inline-block;cursor:default;vertical-align:middle;float:left;font-size:30px;color:#999}
