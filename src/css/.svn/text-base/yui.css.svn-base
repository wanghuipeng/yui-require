.header {text-align: center;background-color: #2bb7ab;color: #fff;height: 45px;line-height: 45px;position: fixed;width: 100%;font-size: 20px;z-index: 100;}
.header:before{content: "";height: 45px;width: 100%;position: absolute;}
.header .icon-return{background: url(../images/return.png) no-repeat;width: 30px;height: 30px;display: inline-block;position: absolute;left: 5px;top: 7px;}
.header .menu{color: #fff;font-size: 14px;position: absolute;right: 5px;top: -1px;}
.header .menu .icon-menu{background: url(../images/menu.png) no-repeat;width: 32px;height: 32px;display: inline-block;vertical-align: middle;}
.header.relative:before{position: relative;}
.pull-right{float: right;}
.pull-left{float: left;}
.over-hide{overflow: hidden;}
/*单行文本省略*/
.hideLine-1{overflow:hidden;text-overflow:ellipsis;display:-webkit-box;-webkit-line-clamp:1;-webkit-box-orient:vertical;white-space:normal}
/*多行文本省略*/
.hideLine-2{overflow:hidden;text-overflow:ellipsis;display:-webkit-box;-webkit-line-clamp:2;-webkit-box-orient:vertical;white-space:normal}
/*clearfix*/
.clearfix:after{visibility:hidden;display:block;font-size:0;content:".";clear:both;height:0}
.clearfix{*zoom:1}
.clearfix{display:inline-block}
.clearfix{display:block}
/*grid*/
.grid{width: 100%;content:"";display:table; clear:both;zoom: 1;}
.grid .cell{height: auto;display: inline-block;float: left;border:1px solid #e2e2e2;border-left: none;border-top: none;text-align: center;padding: 15px 0;background-color: #fff;border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;-webkit-border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;border-width: 0 1px 1px 0;}
.grid .cell:active{background-color: #f3f3f3;}
.grid .cell-icon{vertical-align: middle;display: inline-block;margin-bottom: 5px;}
.grid .cell-p{overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
.grid a{color: inherit;}
.grid.grid-3 .cell{width: 33%;}
/*box*/
.box{padding: 10px 15px;background-color: #fff;line-height: 1.1;}
/*title*/
.yui-title{padding: 0px 15px;background-color: #eee;font-weight: 400;height: 42px;line-height: 42px;border-bottom: 1px solid #ddd;border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;-webkit-border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;border-width: 0 0 1px 0;}
/*button*/
a.btn{display: inline-block;}
.btn{-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;font-size: 14px;height: 36px;line-height: 34px;padding: 0px 12px;border-radius: 4px;border: 1px solid transparent;margin: 5px auto;text-align: center;cursor: pointer;color: #666;background-color: transparent;-webkit-transition: background-color .3s ease-out,border-color .3s ease-out;transition: background-color .3s ease-out,border-color .3s ease-out;}
.btn[disabled],.btn.btn-disabled{opacity: 0.45;cursor: not-allowed;}
.btn-default,.btn-default[disabled]:hover,.btn-default.btn-disabled:hover,.btn-default[disabled]:focus,.btn-default.btn-disabled:focus,.btn-default[disabled]:active,.btn-default.btn-disabled:active{background-color: #fff;border-color: #e2e2e2;color: #666;}
.btn-default:hover,.btn-default:focus{background-color: #f2eeee;border-color: #ddd;}
.btn-default:active,.btn-default.active{background-color: #e0e0e0;border-color: #ccc;}
.btn-primary,.btn-primary[disabled]:hover,.btn-primary.btn-disabled:hover,.btn-primary[disabled]:focus,.btn-primary.btn-disabled:focus,.btn-primary[disabled]:active,.btn-primary.btn-disabled:active{background-color: #2bb7ab;border-color: #2bb7ab;color: #fff;}
.btn-primary:hover,.btn-primary:focus{background-color: #4ccec2;border-color: #4ccec2;}
.btn-primary:active,.btn-primary.active{background-color: #34b0a5;border-color:#34b0a5;}
.btn-warn,.btn-warn[disabled]:hover,.btn-warn.btn-disabled:hover,.btn-warn[disabled]:focus,.btn-warn.btn-disabled:focus,.btn-warn[disabled]:active,.btn-warn.btn-disabled:active{background-color: #f0ad4e;border-color: #eea236;color: #fff;}
.btn-warn:hover,.btn-warn:focus{background-color: #ec971f;border-color: #d58512;}
.btn-warn:active,.btn-warn.active{background-color: #d58512;border-color: #985f0d;}
.btn-success,.btn-success[disabled]:hover,.btn-success.btn-disabled:hover,.btn-success[disabled]:focus,.btn-success.btn-disabled:focus,.btn-success[disabled]:active,.btn-success.btn-disabled:active{background-color:#5cb85c;border-color:#4cae4c;color: #fff;}
.btn-success:hover,.btn-success:focus{background-color:#419641;border-color:#249624;}
.btn-success:active,.btn-success.active{background-color:#449d44;border-color:#398439;}
.btn-info,.btn-info[disabled]:hover,.btn-info.btn-disabled:hover,.btn-info[disabled]:focus,.btn-info.btn-disabled:focus,.btn-info[disabled]:active,.btn-info.btn-disabled:active{background-color:#5bc0de;border-color:#46b8da;color: #fff;}
.btn-info:hover,.btn-info:focus{background-color:#31b0d5;border-color:#269abc;}
.btn-info:active,.btn-info.active{background-color:#269abc;border-color:#1b6d85;}
.btn-danger,.btn-danger[disabled]:hover,.btn-danger.btn-disabled:hover,.btn-danger[disabled]:focus,.btn-danger.btn-disabled:focus,.btn-danger[disabled]:active,.btn-danger.btn-disabled:active{background-color:#d9534f;border-color:#d43f3a;color: #fff;}
.btn-danger:hover,.btn-danger:focus{background-color:#c9302c;border-color:#ac2925;}
.btn-danger:active,.btn-danger.active{background-color:#ac2925;border-color:#761c19;}
.btn-block{display: block;width: 100%;}
.btn-link{border-radius: 0;}
.btn-link:hover,.btn-link:focus{color: #2bb7ab;text-decoration: underline;}
.btn-mini{font-size: 12px;height: 28px;line-height: 26px;}
/*list*/
.list .access:active,.media.access:active{background-color: #f3f3f3;}
.list .list-item{background-color: #fff;width: 100%;content:"";display:table; clear:both;zoom: 1;padding: 10px 15px;box-sizing: border-box;border-bottom: 1px solid #e2e2e2;line-height: 2.1;height: 20px;border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;-webkit-border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;border-width: 0 0 1px 0;}
.list .list-item.list-divider{background-color: #f3f3f3;padding: 5px 15px;}
.list .list-icon{width: 30px;height: 30px;background-color: #e2e2e2;display: inline-block;vertical-align: middle;margin-right: 5px;float: left;background: url(../images/dota.jpg) no-repeat;background-size: cover;background-position: center center;}
