/*switch*/
.yui-switch-cell{line-height: 30px !important;}
.yui-switch{cursor: pointer;width: 65px;background-color: #fff;height: 26px;border-radius: 30px;position: relative;border: 1px solid #ddd;box-shadow: 2px 3px 4px #E3E3E3;}
.yui-switch .yui-switch-handle{position: absolute;left: -2px;top: 1;width: 26px;height: 26px;background-color: #fff;border-radius: 30px;-webkit-box-shadow: 0 2px 5px rgba(0,0,0,.4);box-shadow: 0 2px 5px rgba(0,0,0,.4);-webkit-transition: .2s ease-in-out;transition: .2s ease-in-out;}
.yui-switch.in{background-color: #2bb7ab;border: 2px solid #2bb7ab;}
.yui-switch.in .yui-switch-handle{left: inherit;right: 0px;top: 0px;width: 25px;height: 25px;-webkit-box-shadow: 0 2px 5px rgba(0,0,0,.4);box-shadow: 0 2px 5px rgba(0,0,0,.4);}
.yui-switch-mini{width: 45px;}
.yui-switch-cont{position: relative;}
.yui-switch-cont:before{position: absolute;left: 12px;content: "ON";color: #fff;top: -2px;color: #fff;}
.yui-switch-cont.in:after{display: none;}
.yui-switch-cont:after{position: absolute;right: 8px;content: "OFF";color: #fff;top: -2px;color: #2bb7ab;}
/*accordion*/
.box-outer{margin:15px;}
.accordion>.list>.list-item{padding: 0;}
.accordion{border: 1px solid #e2e2e2;
border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;
-webkit-border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;
border-width:1px;}
.accordion .list .list-item:last-child{border: none;}
.accordion .accordion-header{padding: 0;}
.accordion .accordion-header a{width: 100%;display: inline-table;padding:10px 15px;box-sizing: border-box;}
.accordion .active a{background-color: #f3f3f3;border-bottom: 1px solid #e2e2e2;border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;-webkit-border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;border-width: 0 0 1px 0;}
.accordion .accordion-header .list-arrow-d{transform: rotate(90deg);-ms-transform: rotate(90deg);-webkit-transform: rotate(90deg);transition: transform 0.3s;-webkit-transition: transform 0.3s;}
.accordion .active .list-arrow-d{transform: rotate(-90deg);-ms-transform: rotate(-90deg);-webkit-transform: rotate(-90deg);}	
.accordion .accordion-box{display: none;}
.accordion .active .accordion-box{display: block;}
/*indexListDefault*/
.slelected-letter{width:60px;height:60px;background-color:rgba(40,40,40,.35);text-align:center;line-height:60px;border-radius:100%;position:fixed;top:50%;left:50%;margin-left:-30px;margin-top:-30px;font-size:28px}
.indexList{position:absolute;left:0;top:88px;width:100%;overflow:auto}
.title-fix{position:fixed;width:100%;left:0;top:45px;z-index:10;line-height: 45px;padding:0 15px;}
/*imgBeforeLoad*/
.loading{height:100%;width:100%;display:block;background:-webkit-linear-gradient(#ae006b,#51006a);background:-o-linear-gradient(#ae006b,#51006a);background:-moz-linear-gradient(#ae006b,#51006a);background:linear-gradient(#ae006b,#51006a)}
.spinner{position:absolute;top:38%;left:50%;margin-left:-150px;width:300px;text-align:center}
.spinner>div{width:20px;height:20px;background-color:#F89133;border-radius:100%;display:inline-block;-webkit-animation:bouncedelay 1.4s infinite ease-in-out;animation:bouncedelay 1.4s infinite ease-in-out;-webkit-animation-fill-mode:both;animation-fill-mode:both;color: white;padding: 1px;}
.spinner .bounce1{-webkit-animation-delay:-.32s;animation-delay:-.32s}
.spinner .bounce2{-webkit-animation-delay:-.16s;animation-delay:-.16s}
/*dragDrop*/
.relative{position: relative;}
.dragDrop{position: absolute;top: 10px;left: 15px;z-index: 100;}
/*innerScroll*/
.innerScrollVert{max-height: 300px;overflow: auto;}
.innerScrollHoriz,.innerScrollHoriz ul{overflow-x: scroll;white-space: nowrap;}
.innerScrollHoriz ul li{white-space: nowrap;width: 100px !important;float: none;  box-sizing: border-box;}
.innerScrollElastic{overflow: hidden;}
.innerScrollElastic .tab-contents{width:200%;overflow:hidden;}
.innerScrollElastic .tab-contents .tab-item{width: 50%;float: left;box-sizing: border-box;display: block;}
/*returnTop*/
.returnTop{animation:translateRotate .8s;-webkit-animation:translateRotate .8s;-ms-animation:translateRotate .8s;-moz-animation:translateRotate .8s;-o-animation:translateRotate .8s;background: url(../images/returnTop.png) no-repeat;width: 45px;height: 45px;position: fixed;bottom: 30px;right: 15px;display: none;}
@keyframes translateRotate
{
	0%   { right:-60px; bottom:30px;-webkit-transform: rotate(360deg) scale(1);-moz-transform: rotate(360deg) scale(1);-o-transform: rotate(360deg) scale(1);-ms-transform: rotate(360deg) scale(1);transform: rotate(360deg) scale(1);}
	25%  { right:-35px; bottom:30px;-webkit-transform: rotate(270deg) scale(1);-moz-transform: rotate(270deg) scale(1);-o-transform: rotate(270deg) scale(1);-ms-transform: rotate(270deg) scale(1);transform: rotate(270deg) scale(1);}
	50%  { right:-10px; bottom:30px;-webkit-transform: rotate(180deg) scale(1);-moz-transform: rotate(180deg) scale(1);-o-transform: rotate(180deg) scale(1);-ms-transform: rotate(180deg) scale(1);transform: rotate(180deg) scale(1);}
	75%  { right:0px; bottom:30px;-webkit-transform: rotate(90deg) scale(1);-moz-transform: rotate(90deg) scale(1);-o-transform: rotate(90deg) scale(1);-ms-transform: rotate(90deg) scale(1);transform: rotate(90deg) scale(1);}
	100%  { right:15px; bottom:30px;-webkit-transform: rotate(0deg) scale(1);-moz-transform: rotate(0deg) scale(1);-o-transform: rotate(0deg) scale(1);-ms-transform: rotate(0deg) scale(1);transform: rotate(0deg) scale(1);}
}
@-webkit-keyframes translateRotate /* Safari and Chrome */
{
	0%   { right:-60px; bottom:30px;-webkit-transform: rotate(360deg) scale(1);-moz-transform: rotate(360deg) scale(1);-o-transform: rotate(360deg) scale(1);-ms-transform: rotate(360deg) scale(1);transform: rotate(360deg) scale(1);}
	25%  { right:-35px; bottom:30px;-webkit-transform: rotate(270deg) scale(1);-moz-transform: rotate(270deg) scale(1);-o-transform: rotate(270deg) scale(1);-ms-transform: rotate(270deg) scale(1);transform: rotate(270deg) scale(1);}
