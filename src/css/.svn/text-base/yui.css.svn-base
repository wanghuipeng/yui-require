/*timeline*/
/*时间轴（水平）*/
.steps-hor{background-color:#fff;padding:40px 30px 15px 30px;position:relative;height:40px;border-bottom-left-radius:5px;border-bottom-right-radius:5px}
.steps-hor .ul-1{width:100%;height:5px;background-color:#ffe451}
.steps-hor .ul-1 li{width:25%;display:inline-block;float:left;height:5px;position:relative;margin-top: -5px;}
.steps-hor .ul-3 li:before{width:9px;height:9px;display:inline-block;content:"";position:absolute;left:50%;top:-7px;border-radius:20px;background-color:#ff865c;margin-left:-4.5px}
.steps-hor .ul-3{width:100%;position:relative;left:-18px}
.steps-hor .ul-3 li{display:inline-block;text-align:center;color:#999;height:30px;position:absolute;top:0;font-size:12px;width:60px;margin-left:-13px}
.steps-hor .ul-3 li:nth-child(1){left:0}
.steps-hor .ul-3 li:nth-child(2){left:25%}
.steps-hor .ul-3 li:nth-child(3){left:50%}
.steps-hor .ul-3 li:nth-child(4){left:75%}
.steps-hor .ul-3 li:nth-child(5){left:100%}
.steps-hor .ul-3 li p:first-child{margin-top:5px}
.steps-hor .ul-2{width:0;height:5px;background-color:#ff865c;border-radius:5px;position:relative;margin-top: -5px;}
.orange-bubble{font-size:10px;background-color:#ff865c;border-radius:5px;display:inline-block;position:absolute;right:0;bottom:18px;width:54px;text-align:center;padding:1px 2px 2px 2px;color:#fff;margin-right:-22px}
.orange-bubble:before{position:absolute;bottom:-6px;width:0;height:0;border-left:6px solid transparent;border-right:6px solid transparent;border-top:6px solid #ff865c;content:"";display:block;left:50%;margin-left:-3px}
