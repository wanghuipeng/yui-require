/*upload*/
input[type="file"]{appearance: none;-webkit-appearance: none;-moz-appearance: none;}
.upload{position: relative;margin:15px;}
.upload .icon-upload{position: absolute;width: 63px;height: 63px;position: absolute;left: 0;top: 0;font-size: 40px;text-align: center;line-height: 65px;border: 1px solid #ccc;color: #ccc;font-weight:lighter;}
.upload .icon-upload:before{content:"";position:absolute;left:16px;top:18px;background:url(../images/camera.png) no-repeat;display:inline-block;width:30px;height:30px;background-size:30px}
.upload  input[type="file"]{opacity: 0;width: 65px;height: 65px;position: absolute;left: 0;top: 0;z-index: 10;}
/*table*/
.yui-table{width: 100%;border:1px solid #ccc;border-left: none;border-top: none;}
.yui-table tr th,.yui-table tr td{text-align: center;padding: 8px 12px;border-left: 1px solid #ccc;border-top: 1px solid #ccc;line-height: 1;}
.ok{width: 25px;height: 25px;border-radius:25px;background:url(../images/ok.png) 0px 0px no-repeat;background-size:25px;display: inline-block;vertical-align: middle;}
.ok.ok-1{background-position:0px 0px;}
.ok.ok-2{background-position:0px -45px;}
.ok-square{cursor: pointer;width: 20px;height: 20px;background:url(../images/ok-square.png) 0px 0px no-repeat;background-size:25px;display: inline-block;vertical-align: middle;}
.ok-square.ok-1{background-position:1px 0px;}
.ok-square.ok-2{background-position:2px -24px;}
/*range*/
.range{position:relative;width:100%;height:2px;margin:15px 0;padding:0;cursor:pointer;border:0;border-radius:3px;outline:0;background-color:#999;-webkit-appearance:none!important;-webkit-tap-highlight-color:transparent}
.range::-webkit-slider-thumb{width:28px;height:28px;border-color:#0062cc;border-radius:50%;background-color:#007aff;background-clip:padding-box;-webkit-appearance:none!important}
.range-num input{border:none;font-size:16px;display:inline-block;width:30px}
/*popMenu*/
.yui-translayer{background:0 0;position:fixed;bottom:0;left:0;cursor:pointer;width:100%;height:100%;z-index:300}
.yui-layer{background:#000;filter:alpha(opacity=30);-moz-opacity:.3;opacity:.3;position:fixed;bottom:0;left:0;cursor:pointer;width:100%;height:100%;z-index:300}
.pop-menu{position:absolute;width:80px;right:3px;top:46px;border-radius:5px;background:rgba(0,0,0,.4);display:none;z-index:400}
.pop-menu ul li{display:block;border-bottom:1px solid #ddd;color:#fff;height:30px;line-height:30px;font-size:12px;text-align:center;border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;-webkit-border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;border-width: 0 1px 1px 0;}
.pop-menu:before{content:"";position:absolute;top:-8px;right:5px;border-left:8px solid transparent;border-right:8px solid transparent;border-bottom:8px solid rgba(0,0,0,.4)}
.side-menu{background: #fff;overflow: auto;width: 70%;position: fixed;right: -70%;top: 0;height: 100%;z-index: 400;transition:right .4s;-webkit-transition:right .4s;-moz-transition:right .4s;-o-transition:right .4s;}
.side-menu.in{right: 0;}
.side-menu .list-label{width: 90%;}
.bottom-menu{position: fixed;bottom: -130px;left: 0;width: 100%;text-align: center;z-index: 400;transition:bottom .4s;-webkit-transition:bottom .4s;-moz-transition:bottom .4s;-o-transition:bottom .4s;}
.bottom-menu.in{bottom: 10px;}
.bottom-menu ul,.bottom-menu .bottom-btn{width: 95%;border-radius: 5px;background-color: #fff;margin: 0 auto;color:#2bb7ab;}
.bottom-menu .bottom-btn{height: 40px;line-height: 40px;margin-top: 10px;font-weight: bold;font-size: 16px;}
.bottom-menu ul li{color:#2bb7ab;border-top: 1px solid #ddd;height: 40px;line-height: 40px;font-size: 16px;}
.bottom-menu ul li:first-child{border-top: none;}
.yui-select-wrapper{width: 100%;display: flex;flex-direction:row;height: 35px;line-height: 35px;background-color: #fff;}
.yui-select-wrapper.in{position: fixed;z-index: 400;width: 100%;left: 0;top: 0;}
.yui-select-wrapper>li{position: relative;flex-flow: 0;width: 100%;display: inline-block;text-align: center;border-left: 1px solid #ddd;border-bottom: 1px solid #ddd;white-space: nowrap;overflow: hidden;text-overflow: ellipsis;padding: 0 4%;}
.yui-select-wrapper>li:first-child{border-left: none;}
.yui-select-wrapper>li:after{content: "";top: 3px;}
.yui-select-wrapper .yui-select{display: none;position: fixed;z-index: 400;width: 100%;left: 0;top: 35px;background-color: #fff;}
.yui-select-wrapper .yui-select>ul{overflow: auto;max-height: 286px;}
/*modals*/
.yui-modal-overlay{background-color: rgba(0,0,0,.4);height: 100%;width: 100%;position: fixed;top: 0;left: 0;z-index: 999;}
.yui-modal{display: none;background-color: #fff;width:270px;height: auto;border: 1px solid rgba(0,0,0,.2);border-radius:8px;position: fixed;z-index: 1000;left: 50%;top: 50%;transform:translate(-50%,-50%);-ms-transform:translate(-50%,-50%);-moz-transform:translate(-50%,-50%);-webkit-transform:translate(-50%,-50%);-o-transform:translate(-50%,-50%);}
.yui-modal.in{display: block;}
.yui-modal-content{padding:15px;}
.yui-modal-content .yui-modal-title{font-size: 18px;text-align: center;font-weight: 500;color: #333;}
.yui-modal-content .yui-modal-text{text-align: center;margin-top: 5px;color: #999;max-height: 200px;overflow: auto;}
.yui-modal-content .yui-modal-input{margin-top: 5px;}
.yui-modal-btnGroup{border-top:1px solid rgba(0,0,0,.2);}
.yui-modal-btnGroup ul li{border-left: 1px solid rgba(0,0,0,.2);padding: 3px 0;}
.yui-modal-btnGroup ul li .btn{margin: 0;color: #2bb7ab;}
.yui-modal-btnGroup ul li:first-child{border-left: none;}
.yui-message{display: none;background-color: rgba(0,0,0,.4);color: #fff;text-align: center;border-radius:12px;padding: 15px;position: fixed;z-index: 1000;left: 50%;top: 50%;transform:translate(-50%,-50%);-ms-transform:translate(-50%,-50%);-moz-transform:translate(-50%,-50%);-webkit-transform:translate(-50%,-50%);-o-transform:translate(-50%,-50%);}
.yui-message.in{display: block;}
/*range*/
.range{position:relative;width:100%;height:2px;margin:15px 0;padding:0;cursor:pointer;border:0;border-radius:3px;outline:0;background-color:#999;-webkit-appearance:none!important;-webkit-tap-highlight-color:transparent}
.range:-webkit-slider-thumb{width:28px;height:28px;border-color:#0062cc;border-radius:50%;background-color:#007aff;background-clip:padding-box;-webkit-appearance:none!important}
.range-num input{border:none;font-size:16px;display:inline-block;width:30px;color: #666;}
/*slideDel*/
.slideDel{position:relative;overflow:hidden;-webkit-transform:translate(0,0);-webkit-transition:all .3s linear;-moz-transform:translate(0,0);-webkit-transition:all .3s linear;transform:translate(0,0);-webkit-transition:all .3s linear}
.slideDel .del{background-color:#d9534f;color:#fff;text-align:center;position:absolute;z-index:10;right:-25%;width:25%;top:0;padding:21px 0}
.slideDel.selected{overflow:inherit;-webkit-transform:translate(-25%,0);-webkit-transition:all .3s linear;-moz-transform:translate(-25%,0);-webkit-transition:all .3s linear;transform:translate(-25%,0);-webkit-transition:all .3s linear}
/*badge*/
.yui-badge{display:inline-block;min-width:10px;padding:3px 7px;font-size:12px;font-weight:700;line-height:1;color:#fff;text-align:center;white-space:nowrap;vertical-align:baseline;background-color:#777;border-radius:10px}
.btn-default .yui-badge{background-color:#333}
.btn-primary .yui-badge{background-color:#fff;color:#4ccec2}
.btn-warn .yui-badge{background-color:#fff;color:#f0ad4e}
.btn-success .yui-badge{background-color:#fff;color:#5cb85c}
.btn-danger .yui-badge{background-color:#fff;color:#d9534f}
.btn-info .yui-badge{background-color:#fff;color:#5bc0de}
/*label*/
.yui-label{display:inline;padding:.2em .6em .3em;font-size:75%;font-weight:700;line-height:1;color:#fff;text-align:center;white-space:nowrap;vertical-align:baseline;border-radius:.25em}
.yui-label-default{background-color:#777}
.yui-label-primary{background-color:#4ccec2}
.yui-label-success{background-color:#5cb85c}
.yui-label-warn{background-color:#f0ad4e}
.yui-label-danger{background-color:#d9534f}
.yui-label-info{background-color:#5bc0de}
/* @media */
@media only screen and (min-width: 1220px) {
	.search-input .cancel{text-align: center;text-indent: 1%;}
}
@media only screen and (min-width: 360px) and (max-width:1219px) {
	.search-input .cancel{text-align: center;text-indent: 3%;}
}
@media only screen and (min-width: 1200px) {
	.grid.grid-3 .cell{width: 33.25%;}
}
@media only screen and (min-width: 768px) and (max-width: 1199px) {
	.grid.grid-3 .cell{width: 33.2%;}
}
@media only screen and (min-width: 430px) and (max-width: 767px) {
	.grid.grid-3 .cell{width: 33.1%;}
}
@media (max-width:600px){.water-item{width:120px;height:80px}
}
@media (min-width:340px){.orderStep li:before{right: 90%;}
}
@media (min-width:319px) and (max-width:360px){.grid .cell{padding: 10px 0;}
}
@-webkit-keyframes bouncedelay{0%,100%,80%{-webkit-transform:scale(0)}
40%{-webkit-transform:scale(1)}
}
@keyframes bouncedelay{0%,100%,80%{transform:scale(0);-webkit-transform:scale(0)}
40%{transform:scale(1);-webkit-transform:scale(1)}
}